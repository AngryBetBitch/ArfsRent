                <div class="containerWest2">
                    <div class="box-West7">
                        <div class="card text-dark bg-warning mb-3">
                            <div class="card-body">
                                <canvas id="myChartYaro"></canvas>
                            </div>
                        </div>
                    </div>
                    <div class="box-West8">
                        <div class="card text-dark bg-warning mb-3">
                            <div class="card-body">
                                <table class="table table-bordered">
                                    <tr>
                                        <th>Дома</th>
                                        <th>ГП</th>
                                        <th>Регион</th>
                                        <th>AAB</th>
                                    </tr>

                                    @for (int i = 0; i < Model.publicListOHouses.Count; i++)
                                    {
                                        <tr>
                                            <th>@(Model.publicListOHouses[i].Split("|")[0])</th>
                                            <th>@(Model.publicListOHouses[i].Split("|")[1])</th>
                                            <th>@(Model.publicListOHouses[i].Split("|")[2])</th>
                                            <th>@(Model.publicListOHouses[i].Split("|")[3])</th>
                                        </tr>
                                    }
                                </table>
                                <canvas id="ChartWestD"></canvas>
                            </div>
                        </div>
                    </div>
                    <div class="box-West9">
                        <div class="card text-dark bg-warning mb-3">
                            <div class="card-body">
                                <table class="table table-sm">
                                    <tr>
                                        <th>Ticket</th>
                                        <th>Reg</th>
                                        <th>AAB</th>
                                        <th>CREATED</th>
                                        <th>H</th>
                                    </tr>

                                    @for (int i = 0; i < Model.publicListOfTickets.Count; i++)
                                    {
                                        <tr>
                                            <th>@(Model.publicListOfTickets[i].Split("|")[0])</th>
                                            <th>@(Model.publicListOfTickets[i].Split("|")[1])</th>
                                            <th>@(Model.publicListOfTickets[i].Split("|")[2])</th>
                                            <th>@(Model.publicListOfTickets[i].Split("|")[3])</th>
                                            <th>@(Model.publicListOfTickets[i].Split("|")[4])</th>
                                        </tr>
                                    }
                                </table>
                            </div>
                        </div>
                    </div>  
                    <div>@Model.DtLoad</div>
                        </div>

                <script>
    var ctx = document.getElementById('myChart').getContext('2d');
    var myChart = new Chart(ctx, {
        type: 'line',
        data: {
            labels: @Html.Raw(Model.SumDates),
            datasets: [{
                data: @(Model.LARFSW),
                label: "Запад",
                borderColor: "#3e95cd",
                backgroundColor: "#7bb6dd",
                fill: false,
            },
                     {
                        labels: @Html.Raw(Model.SumDates),
                    data: @(Model.LineWest) ,
                        label: "arfs",
                        borderColor: "#000000",
                        backgroundColor: "#ffffff",
                        borderWidth: 1 ,
                        borderDash: [5, 5],
                        type: 'line',
                         fill: false,
                         pointRadius: 0
                    }
            ]
        },
    });
                </script>

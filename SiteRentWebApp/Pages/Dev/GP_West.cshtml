            type: 'doughnut',
            data: {
                labels: @Html.Raw(Model.SumFilWest),
                datasets: [{
                    data: @(Model.SumGPWest),
                    borderColor: [
                        "#3cba9f",
                        "#ffa500",
                        "#c45850",

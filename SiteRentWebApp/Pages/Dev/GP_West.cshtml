@page

@model GP_West
@{ Layout = "/Pages/Shared/_TestLayout.cshtml"; }

<h1>Rent FTTB West</h1>
<div class="containerWest">
    <div class="box-WestCheck">
        <div class="card text-dark bg-warning mb-3">
            <div class="card-body">
                <canvas id="myChart"></canvas>
            </div>
        </div>
    </div>

    <div class="box-West">
        <div class="card text-dark bg-warning mb-3">
            <div class="card-body">
                <canvas id="myChartArch"></canvas>
            </div>
        </div>
    </div>
